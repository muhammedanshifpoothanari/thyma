	function subscribe(){
  let sub=document.getElementById('email').value;
  const xhttp = new XMLHttpRequest();
  $.ajax({
    url:'/sub',
    type:'post',
    dataType:'json',
    contentType:'application/json',
    data:JSON.stringify({sub:sub}),
    success:(data)=>{
		console.log(data);
		swal({
                  title: "Error in Subscribing!",
                  text: `subscribing to ${sub} occured an error,please try again`,
                  icon: "error",                  
                  position: "center",
            })
		if(data.msg=='subscribed'){
        	console.log('==============---------------------------==================');
			swal({
                  title: "Subscribed",
                  text: `${sub} is subscribed successfully`,
                  icon: "success",                  
                  position: "center",
            });
		}
		else if(data.msg=='already_subscribed'){
            console.log('==============-----------///][][][][][][][///----------------==================');
            swal({
                  title: "Subscribed",
                  text: `${sub} is already  subscribed`,
                  icon: "success",                  
                  position: "center",
            });
    }
		else{
			console.log('==============---///////////--------//////////----------------==================');
			swal({
                  title: "Error in Subscribing!",
                  text: `subscribing to ${sub} occured an error,please try again`,
                  icon: "error",                  
                  position: "center",
            })
		}
		
      }
  })
}



